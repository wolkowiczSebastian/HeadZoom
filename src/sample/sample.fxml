<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TabPane prefHeight="0.0" prefWidth="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <tabs>
            <Tab>
               <content>
                  <BorderPane>
                     <top>
                        <VBox>
                           <HBox alignment="CENTER" spacing="10">
                              <padding>
                                 <Insets bottom="10" top="10" />
                              </padding>
                              <Button fx:id="ButtonOpenWebBrowoser" mnemonicParsing="false" onAction="#ButtonConnectBrowserPressed" text="Connect Browoser" />
                              <Button fx:id="cameraButton" alignment="center" onAction="#startCamera" text="Start camera" />
                           </HBox>
                           <Separator />
                        </VBox>
                     </top>
                     <center>
                        <VBox alignment="CENTER">
                           <padding>
                              <Insets left="10" right="10" />
                           </padding>
                           <ImageView fx:id="originalFrame" fitHeight="1.0" fitWidth="178.0" />
                        </VBox>
                     </center>
                     <bottom>
                        <VBox>
                           <children>
                              <Label text="Set Tolerance" />
                              <HBox alignment="CENTER">
                                 <padding>
                                    <Insets bottom="25" left="25" right="25" top="25" />
                                 </padding>
                                 <children>
                                    <Slider blockIncrement="5.0" showTickLabels="true" showTickMarks="true" value="5.0" />
                                           <Button mnemonicParsing="false" text="Set Position" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </bottom>
                  </BorderPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
